{
  "name": "packai",
  "displayName": "PackAI",
  "description": "Coordinate Claude, Copilot, and Codex agents to build web projects together. Describe your project, get an intelligent execution plan, and let three AI agents collaborate â€” each assigned to the tasks they do best.",
  "version": "0.1.1",
  "publisher": "premxai",
  "license": "MIT",
  "icon": "media/icon.png",
  "galleryBanner": {
    "color": "#1e1e2e",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/premxai/PackAI.git"
  },
  "bugs": {
    "url": "https://github.com/premxai/PackAI/issues"
  },
  "homepage": "https://github.com/premxai/PackAI#readme",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "AI",
    "Chat",
    "Programming Languages",
    "Machine Learning"
  ],
  "keywords": [
    "ai",
    "agent",
    "orchestrator",
    "claude",
    "copilot",
    "codex",
    "web development",
    "multi-agent",
    "scaffolding",
    "code generation",
    "workflow",
    "automation"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "packai.startProject",
        "title": "Start Project",
        "category": "PackAI",
        "icon": "$(rocket)"
      },
      {
        "command": "packai.openDashboard",
        "title": "Open Dashboard",
        "category": "PackAI",
        "icon": "$(dashboard)"
      },
      {
        "command": "packai.openSettings",
        "title": "Open Settings",
        "category": "PackAI",
        "icon": "$(gear)"
      },
      {
        "command": "packai.pauseOrchestration",
        "title": "Pause Orchestration",
        "category": "PackAI",
        "icon": "$(debug-pause)",
        "enablement": "packai.hasActiveSessions"
      },
      {
        "command": "packai.resumeOrchestration",
        "title": "Resume Orchestration",
        "category": "PackAI",
        "icon": "$(debug-continue)",
        "enablement": "packai.hasActiveSessions"
      },
      {
        "command": "packai.cancelOrchestration",
        "title": "Cancel Orchestration",
        "category": "PackAI",
        "icon": "$(stop-circle)",
        "enablement": "packai.hasActiveSessions"
      },
      {
        "command": "packai.viewSessionDetails",
        "title": "View Session Details",
        "category": "PackAI",
        "icon": "$(list-unordered)"
      },
      {
        "command": "packai.retryTask",
        "title": "Retry Task",
        "category": "PackAI",
        "icon": "$(refresh)"
      },
      {
        "command": "packai.resolveConflict",
        "title": "Resolve Conflict",
        "category": "PackAI",
        "icon": "$(git-merge)"
      },
      {
        "command": "packai.browseTemplates",
        "title": "Browse Templates",
        "category": "PackAI",
        "icon": "$(file-code)"
      },
      {
        "command": "packai.createTemplate",
        "title": "Create Template from Plan",
        "category": "PackAI",
        "icon": "$(add)"
      },
      {
        "command": "packai.importTemplate",
        "title": "Import Template",
        "category": "PackAI",
        "icon": "$(cloud-download)"
      },
      {
        "command": "packai.exportTemplate",
        "title": "Export Template",
        "category": "PackAI",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "packai.configureAgents",
        "title": "Configure Agent Preferences",
        "category": "PackAI",
        "icon": "$(settings-gear)"
      },
      {
        "command": "packai.configureApproval",
        "title": "Configure Approval Rules",
        "category": "PackAI",
        "icon": "$(shield)"
      },
      {
        "command": "packai.resetSettings",
        "title": "Reset Settings to Defaults",
        "category": "PackAI",
        "icon": "$(discard)"
      }
    ],
    "keybindings": [
      {
        "command": "packai.startProject",
        "key": "ctrl+shift+w s",
        "mac": "cmd+shift+w s"
      },
      {
        "command": "packai.openDashboard",
        "key": "ctrl+shift+w d",
        "mac": "cmd+shift+w d"
      },
      {
        "command": "packai.pauseOrchestration",
        "key": "ctrl+shift+w p",
        "mac": "cmd+shift+w p"
      },
      {
        "command": "packai.resumeOrchestration",
        "key": "ctrl+shift+w r",
        "mac": "cmd+shift+w r"
      },
      {
        "command": "packai.browseTemplates",
        "key": "ctrl+shift+w t",
        "mac": "cmd+shift+w t"
      }
    ],
    "viewsContainers": {
      "panel": [
        {
          "id": "packai-dashboard",
          "title": "PackAI",
          "icon": "$(rocket)"
        }
      ]
    },
    "views": {
      "packai-dashboard": [
        {
          "id": "packai.dashboardView",
          "name": "Dashboard",
          "type": "webview"
        }
      ]
    },
    "configuration": {
      "title": "PackAI",
      "properties": {
        "packai.agentPreferences.selectionStrategy": {
          "type": "string",
          "default": "intelligent",
          "enum": [
            "intelligent",
            "roundRobin",
            "preferClaude",
            "preferCopilot",
            "preferCodex"
          ],
          "enumDescriptions": [
            "Automatically select the best agent based on task analysis and benchmarks",
            "Distribute tasks evenly across available agents",
            "Prefer Claude for all tasks (fallback to others if unavailable)",
            "Prefer Copilot for all tasks (fallback to others if unavailable)",
            "Prefer Codex for all tasks (fallback to others if unavailable)"
          ],
          "description": "How the orchestrator selects agents when multiple could handle a task."
        },
        "packai.agentPreferences.costOptimizationLevel": {
          "type": "string",
          "default": "balanced",
          "enum": [
            "economy",
            "balanced",
            "performance"
          ],
          "enumDescriptions": [
            "Minimize cost by preferring cheaper models and fewer API calls",
            "Balance cost and quality for most workflows",
            "Maximize quality by using the most capable models available"
          ],
          "description": "Cost-vs-quality tradeoff for model selection."
        },
        "packai.agentPreferences.maxParallelSessions": {
          "type": "integer",
          "default": 3,
          "minimum": 1,
          "maximum": 10,
          "description": "Maximum number of parallel agent sessions allowed."
        },
        "packai.approval.autoApproveTools": {
          "type": "array",
          "default": [
            "READ",
            "WEB_SEARCH"
          ],
          "items": {
            "type": "string",
            "enum": [
              "READ",
              "CREATE",
              "EDIT",
              "DELETE",
              "TERMINAL",
              "WEB_SEARCH"
            ]
          },
          "description": "Tool types to always auto-approve without prompting."
        },
        "packai.approval.alwaysDenyTools": {
          "type": "array",
          "default": [
            "DELETE"
          ],
          "items": {
            "type": "string",
            "enum": [
              "READ",
              "CREATE",
              "EDIT",
              "DELETE",
              "TERMINAL",
              "WEB_SEARCH"
            ]
          },
          "description": "Tool types to always require manual confirmation."
        },
        "packai.approval.agentTrustLevels": {
          "type": "object",
          "default": {
            "claude": "standard",
            "copilot": "standard",
            "codex": "standard"
          },
          "properties": {
            "claude": {
              "type": "string",
              "enum": [
                "minimal",
                "standard",
                "elevated",
                "full"
              ],
              "default": "standard"
            },
            "copilot": {
              "type": "string",
              "enum": [
                "minimal",
                "standard",
                "elevated",
                "full"
              ],
              "default": "standard"
            },
            "codex": {
              "type": "string",
              "enum": [
                "minimal",
                "standard",
                "elevated",
                "full"
              ],
              "default": "standard"
            }
          },
          "description": "Per-agent trust level. Higher trust means fewer approval interruptions."
        },
        "packai.approval.devContainerMode": {
          "type": "boolean",
          "default": true,
          "description": "Activate permissive approval mode when running in a dev container."
        },
        "packai.approval.productionWorkspace": {
          "type": "boolean",
          "default": false,
          "description": "Mark workspace as production to activate restrictive approval mode."
        },
        "packai.ui.autoOpenDashboard": {
          "type": "boolean",
          "default": true,
          "description": "Automatically open the dashboard when a workflow starts."
        },
        "packai.ui.notificationVerbosity": {
          "type": "string",
          "default": "normal",
          "enum": [
            "silent",
            "minimal",
            "normal",
            "verbose"
          ],
          "enumDescriptions": [
            "No notifications (errors only in output channel)",
            "Only errors and important warnings",
            "Standard notifications for task completion and warnings",
            "Detailed notifications for every agent action"
          ],
          "description": "How chatty the extension is with VS Code notifications."
        },
        "packai.ui.dashboardTheme": {
          "type": "string",
          "default": "auto",
          "enum": [
            "auto",
            "light",
            "dark"
          ],
          "enumDescriptions": [
            "Follow VS Code's current theme",
            "Always use light theme for the dashboard",
            "Always use dark theme for the dashboard"
          ],
          "description": "Dashboard color theme override."
        },
        "packai.ui.activityLogLimit": {
          "type": "integer",
          "default": 100,
          "minimum": 10,
          "maximum": 500,
          "description": "Maximum number of activity log entries displayed in the dashboard."
        },
        "packai.advanced.customTemplatesDirectory": {
          "type": "string",
          "default": "",
          "description": "Path to a directory containing custom workflow templates. Leave empty to use built-in templates."
        },
        "packai.advanced.benchmarkDataPath": {
          "type": "string",
          "default": "",
          "description": "Path to benchmark data JSON file. Leave empty for default (.packai/benchmarks.json)."
        },
        "packai.advanced.sessionTimeoutMs": {
          "type": "integer",
          "default": 300000,
          "minimum": 0,
          "maximum": 3600000,
          "description": "Session timeout in milliseconds. Set to 0 for no timeout."
        },
        "packai.advanced.maxRetries": {
          "type": "integer",
          "default": 3,
          "minimum": 0,
          "maximum": 10,
          "description": "Maximum retry attempts when an agent session fails."
        },
        "packai.advanced.retryBaseDelayMs": {
          "type": "integer",
          "default": 1000,
          "minimum": 100,
          "maximum": 60000,
          "description": "Base delay in milliseconds between retry attempts (with exponential backoff)."
        },
        "packai.advanced.telemetryEnabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable opt-in telemetry to track error types and frequency locally. No code is sent externally."
        },
        "packai.advanced.gitCheckpointEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Automatically create a git commit after each completed phase for easy rollback."
        },
        "packai.advanced.stateCheckpointIntervalMs": {
          "type": "integer",
          "default": 30000,
          "minimum": 5000,
          "maximum": 300000,
          "description": "How often (in milliseconds) to auto-save execution state for crash recovery."
        }
      }
    },
    "chatParticipants": [
      {
        "id": "packai.orchestrator",
        "name": "packai",
        "fullName": "PackAI Orchestrator",
        "description": "Multi-agent web development coordinator",
        "isSticky": true,
        "commands": [
          {
            "name": "scaffold",
            "description": "Generate project structure with optimal agent"
          },
          {
            "name": "component",
            "description": "Create a UI component"
          },
          {
            "name": "api",
            "description": "Build API endpoints"
          },
          {
            "name": "test",
            "description": "Generate and run tests"
          },
          {
            "name": "review",
            "description": "Multi-agent code review"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "tsc -p ./",
    "compile:check": "tsc --noEmit",
    "watch": "tsc -watch -p ./",
    "bundle": "node esbuild.mjs",
    "bundle:watch": "node esbuild.mjs --watch",
    "package": "npm run compile:check && node esbuild.mjs --production",
    "lint": "eslint src --ext ts",
    "pretest": "npm run compile",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "vsix": "npx @vscode/vsce package --no-dependencies",
    "prepublish:check": "npm run compile:check && npm run lint && npm run test"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/vscode": "^1.109.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.25.0",
    "eslint": "^9.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.2.4"
  }
}
